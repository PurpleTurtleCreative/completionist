{
  "name": "completionist",
  "version": "2.1.0",
  "author": "Michelle Blanchette",
  "description": "Asana task management integration for WordPress.",
  "homepage": "https://purpleturtlecreative.com/completionist/",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PurpleTurtleCreative/completionist.git"
  },
  "bugs": {
    "url": "https://github.com/PurpleTurtleCreative/completionist/issues"
  },
  "devDependencies": {
    "@wordpress/scripts": "10.0.0",
    "node-sass": "^5.0.0"
  },
  "scripts": {
    "build": "wp-scripts build && npm run styles",
    "start": "wp-scripts start",
    "bundle": "npm run build && bash ./bundle.sh",
    "styles": "node-sass assets/styles/scss/ -o assets/styles/ --source-map true --output-style compressed",
    "watch:styles": "node-sass assets/styles/scss/ -o assets/styles/ --source-map true --output-style compressed -wr"
  }
}
